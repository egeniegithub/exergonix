function state_form_init(state){
    $(".map_area").hide();
    $("#chart_state").show();
}
function ea_form_init(state){
    $(".map_area").hide();
   
    setCountyEAMap(map_short_ids);
    $("#chart_ea").show();
}
function county_form_init(state){
    
    $(".map_area").hide();
    $("#curtain").show();
    setCountyMap(map_ids);
    $("#chart_county").show();


}
function city_form_init(state){
    $(".map_area").hide();
    $("#chart_city").show();
}


